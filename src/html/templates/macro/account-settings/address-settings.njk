<div class="container address__container tabcontent" id="address">
    <h3>Address settings</h3>
    <form>
        <div class="form-group">
            <label for="acc_firstName">First Name *</label>
            <input type="text" class="form-control" id="acc_firstName">
        </div>
        <div class="form-group">
            <label for="acc_lastName">Last Name *</label>
            <input type="text" class="form-control" id="acc_lastName">
        </div>
        <div class="form-group">
            <label for="country">Country *</label>
            <select class="form-control" id="country">
                <option>Nepal</option>
                <option>India</option>
                <option>Pakistan</option>
                <option>USA</option>
            </select>
        </div>
        <div class="form-group">
            <label for="city" >City *</label>
            <select class="form-control" id="city">
                <option>Kathmandu</option>
                <option>2</option>
                <option>3</option>
                <option>4</option>
                <option>5</option>
            </select>
        </div>
        <div class="form-group">
            <label for="acc_streetName">Street Name/No *</label>
            <input type="text" class="form-control" id="acc_streetName">
        </div>
        <div class="form-group">
            <label for="nearestLandmark">Nearest Landmark *</label>
            <input type="text" class="form-control" id="nearestLandmark">
        </div>
        <div class="form-group">
            <label for="locationType">Location Type *</label>
            <select class="form-control" id="locationType">
                <option>Home</option>
                <option>Business</option>
            </select>
        </div>
        <div class="form-group">

            <label for="mobile">Mobile</label>
            <div class="input-group mb-2">
                <div class="input-group-prepend">
                    <div class="input-group-text">+977</div>
                </div>
                <input type="text" class="form-control" id="mobile" >
            </div>
        </div>
        <div class="form-group">
            <label for="landline">Landline *</label>
            <div class="input-group mb-2">
                <div class="input-group-prepend">
                    <div class="input-group-text">+977</div>
                </div>
                <input type="text" class="form-control" id="landline" >
            </div>
        </div>

        <button type="submit" class="view-cart">Submit</button>
        <button type="submit" class="btn btn-danger">Cancel</button>
    </form>
</div>
